{
  "name": "pourquellelistevoter.be",
  "version": "1.0.0",
  "description": "Site Internet pourquellelistevoter.be",
  "main": "index.js",
  "engines": {
    "node": "10.x.x"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --exec babel-node -- src/server/index.js",
    "build": "npm run build:server && npm run build:next",
    "build:server": "babel --copy-files ./src --out-dir ./dist",
    "build:next": "next build dist",
    "start": "NODE_ENV=production node dist/server/index.js",
    "lint": "eslint \"src/**/*.js\" \"test/**/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "lint:quiet": "npm run lint -- --quiet",
    "prettier": "prettier src/**/*.js test/**/*.js --write",
    "clean": "rm -rf dist src/.next"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xdamman/pourquellelistevoter.be.git"
  },
  "keywords": [
    "communales2018",
    "elections",
    "municipalism",
    "citizens",
    "politics"
  ],
  "author": "Xavier Damman <@xdamman>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xdamman/pourquellelistevoter.be/issues"
  },
  "homepage": "https://github.com/xdamman/pourquellelistevoter.be#readme",
  "dependencies": {
    "@mdx-js/loader": "^0.15.4",
    "@mdx-js/mdx": "^0.15.4",
    "@zeit/next-mdx": "^1.2.0",
    "csv-parser": "^2.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "lodash": "^4.17.11",
    "next": "^7.0.1",
    "next-routes": "^1.4.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-player": "^1.6.6",
    "remark-emoji": "^2.0.1",
    "remark-images": "^0.8.1",
    "semantic-ui-react": "^0.82.5",
    "serve-favicon": "^2.5.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/node": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.9.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-node": "^1.0.2",
    "eslint-plugin-react": "^7.11.1",
    "nodemon": "^1.18.4"
  },
  "nodemonConfig": {
    "watch": [
      "src/lib",
      "src/server",
      "src/routes.js"
    ]
  }
}
